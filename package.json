{
  "name": "react-native-start",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "ENVFILE=.env.prod react-native run-android --mode=release",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:build-prod": "cd android && ENVFILE=.env.prod ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "ios:release": "ENVFILE=.env.prod react-native run-ios --mode=Release",
    "ios:build": "cd ios && xcodebuild -workspace RNProject.xcworkspace -scheme RNProject -configuration Release clean archive -archivePath build/RNProject.xcarchive",
    "ios:build-prod": "cd ios && ENVFILE=.env.prod xcodebuild -workspace RNProject.xcworkspace -scheme RNProject -configuration Release clean archive -archivePath build/RNProject.xcarchive",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "axios": "^1.13.2",
    "react": "19.2.3",
    "react-native": "0.83.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@react-native-community/cli": "20.0.2",
    "@react-native-community/cli-platform-android": "20.0.2",
    "@react-native-community/cli-platform-ios": "20.0.2",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "prettier": "3.7.4",
    "react-native-config": "^1.6.1",
    "react-test-renderer": "19.2.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "22.18.0 // used to init"
  }
}
